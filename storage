select album.id, songs.song_name
from album
inner join album_song
on album.id = album_song.album_id
left join songs
on songs.id = album_song.song_id
where album.id = 2
order by songs.song_name asc;



select album.album_name, songs.song_name, songs.duration
from album
inner join album_song
on album.id = album_song.album_id
left join songs
on songs.id = album_song.song_id
order by songs.song_name asc;
